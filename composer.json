{
    "name": "cookie10codes/vat-checker",
    "description": "Validates and returns company info of VAT Number in Europe",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "vat checker"
    ],
    "support": {
        "docs": "https://github.com/cookie10codes/vat-checker/blob/master/README.md",
        "issues": "https://github.com/cookie10codes/vat-checker/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stefanie Gevaert",
            "homepage": "https://koeketienedesign.be/"
        }
    ],
    "require": {
